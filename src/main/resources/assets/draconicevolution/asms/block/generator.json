{
    "parameters":  {
        "world_to_cycle": ["*", "#fan_speed"],
        "round_cycle":    ["compose", ["R", ["compose", ["*", 4], "#fan_speed"]], "#switch_time"],
        "end_cycle":      ["-", "#round_cycle"]
    },
    "clips":       {
        "default":  "draconicevolution:block/generator/generator_fan@default",
        "starting": ["trigger_positive", "#default", "#end_cycle", "!transition:moving"],
        "moving":   ["apply", "draconicevolution:block/generator/generator_fan@moving", "#world_to_cycle"],
        "stopping": ["trigger_positive", "#moving", "#end_cycle", "!transition:default"]
    },
    "states":      [
        "default", "starting", "moving", "stopping"
    ],
    "transitions": {
        "default":  ["starting", "moving"],
        "starting": ["moving"],
        "moving":   ["stopping", "default"],
        "stopping": "default"
    },
    "start_state": "default"
}